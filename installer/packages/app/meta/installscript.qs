function Component() {}Component.prototype.createOperations = function() {    component.createOperations();        var os = installer.value("os");        // Windows平台    if (os === "win") {        component.addOperation(            "CreateShortcut",            "@TargetDir@/notepeek.exe",            "@StartMenuDir@/NotePeek.lnk",            "workingDirectory=@TargetDir@",            "description=Open Application"        );        component.addOperation(            "CreateShortcut",            "@TargetDir@/notepeek.exe",            "@DesktopDir@/NotePeek.lnk",            "workingDirectory=@TargetDir@",            "description=Open Application"        );    }     // Linux平台（包括X11和Wayland）    else if (os === "x11" || os === "wayland") {        // 创建桌面快捷方式        component.addOperation(            "CreateDesktopEntry",            "@TargetDir@/notepeek",            "@DesktopDir@/NotePeek.desktop",            "Name=NotePeek",            "Exec=@TargetDir@/notepeek",            "Icon=@TargetDir@/icon.png",            "Terminal=false",            "Type=Application",            "Categories=Utility;"        );                // 创建应用程序菜单项        component.addOperation(            "CreateDesktopEntry",            "@TargetDir@/notepeek",            "@HomeDir@/.local/share/applications/NotePeek.desktop",            "Name=NotePeek",            "Exec=@TargetDir@/notepeek",            "Icon=@TargetDir@/icon.png",            "Terminal=false",            "Type=Application",            "Categories=Utility;"        );    }     // macOS平台    else if (os === "macx") {        component.addOperation(            "CreateSymlink",            "@TargetDir@/NotePeek.app",            "@HomeDir@/Applications/NotePeek.app"        );    }}